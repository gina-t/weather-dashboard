{
"compilerOptions": {
  "outDir": "./dist",
  "rootDir": "./src",
  "module": "es2020",
  "target": "es6",
  "moduleResolution": "node",
  "esModuleInterop": true,
  "strict": true,
  "skipLibCheck": true // Add this line to skip type checking of declaration files
},
"include": ["src/**/*"],
"exclude": ["node_modules", "dist"]
}